{
  "name": "lqhuang-blog",
  "version": "0.2.1",
  "description": "lqhuang's blog",
  "homepage": "https://lqhuang.io",
  "bugs": {
    "url": "https://github.com/lqhuang/website/issues"
  },
  "repository": "https://github.com/lqhuang/website",
  "license": "MIT",
  "author": "Lanqing Huang <lqhuang@outlook.com>",
  "type": "module",
  "scripts": {
    "build": "next build && cp -f _redirects out/_redirects",
    "cf-build": "git clone https://github.com/lqhuang/blog --depth 1 && stat blog && pnpm build",
    "clean": "rm -rf ./.next/ ./node_modules/ ./out/",
    "compile": "tsc --noEmit --allowSyntheticDefaultImports false",
    "dev": "next dev -H 0.0.0.0 --turbopack",
    "lint": "next lint",
    "setup": "git clone https://github.com/lqhuang/blog --depth 1",
    "start": "next start --hostname 0.0.0.0",
    "test": "vitest --exclude retired/ src/ tests/",
    "up-deps": "pnpm update -iL",
    "up-greedy": "pnpm update --latest --filter=!@types/node"
  },
  "dependencies": {
    "@fontsource/ia-writer-mono": "^5.2.5",
    "@heroicons/react": "^2.2.0",
    "@mdx-js/mdx": "^3.1.0",
