{
  "name": "lqhuang-website",
  "version": "0.2.0",
  "description": "lqhuang's personal website",
  "homepage": "https://lqhuang.io",
  "repository": "https://github.com/lqhuang/website",
  "author": "Lanqing Huang <lqhuang@outlook.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "tsx node_modules/@11ty/eleventy/cmd.cjs --config=eleventy.config.ts --serve --incremental",
    "build": "eleventy && cp -f _redirects _site/_redirects",
    "start": "eleventy --serve",
    "lint": "eslint",
    "test": "vitest",
    "up-deps": "pnpm update",
    "up-greedy": "pnpm update --latest --filter=!@types/node",
    "clean": "rm -rf _site/",
    "setup": "git clone https://github.com/lqhuang/blog --depth 1",
    "cf-build": "git clone https://github.com/lqhuang/blog --depth 1 && stat blog && pnpm build"
  },
  "dependencies": {
    "@fontsource/ia-writer-mono": "^5.1.0",
    "@fontsource/zilla-slab": "^5.1.1",
    "@shikijs/rehype": "^1.24.4",
    "@t3-oss/env-nextjs": "^0.11.1",
    "date-fns": "^4.1.0",
    "gray-matter": "^4.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rehype-katex": "^7.0.1",
    "rehype-slug": "^6.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "remark-parse": "^11.0.0",
    "remark-reference-links": "^7.0.0",
    "remark-rehype": "^11.1.1",
    "shiki": "^1.24.4",
    "yaml": "^2.6.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "3.0.1-alpha.3",
    "@eslint-react/eslint-plugin": "^1.22.1",
    "@eslint/eslintrc": "^3.2.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "postcss": "^8.4.49",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.3.0",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "vitest": "^2.1.8"
  },
  "pnpm": {
    "overrides": {
      "array-includes": "npm:@nolyfill/array-includes@^1",
      "array.prototype.flat": "npm:@nolyfill/array.prototype.flat@^1",
      "array.prototype.flatmap": "npm:@nolyfill/array.prototype.flatmap@^1",
      "array.prototype.tosorted": "npm:@nolyfill/array.prototype.tosorted@^1",
      "has": "npm:@nolyfill/has@^1",
      "object.assign": "npm:@nolyfill/object.assign@^1",
      "object.entries": "npm:@nolyfill/object.entries@^1",
      "object.fromentries": "npm:@nolyfill/object.fromentries@^1",
      "object.hasown": "npm:@nolyfill/object.hasown@^1",
      "object.values": "npm:@nolyfill/object.values@^1",
      "string.prototype.matchall": "npm:@nolyfill/string.prototype.matchall@^1",
      "array.prototype.findlast": "npm:@nolyfill/array.prototype.findlast@^1",
      "array.prototype.findlastindex": "npm:@nolyfill/array.prototype.findlastindex@^1",
      "es-iterator-helpers": "npm:@nolyfill/es-iterator-helpers@^1",
      "hasown": "npm:@nolyfill/hasown@^1",
      "is-core-module": "npm:@nolyfill/is-core-module@^1",
      "isarray": "npm:@nolyfill/isarray@^1",
      "object.groupby": "npm:@nolyfill/object.groupby@^1",
      "safe-buffer": "npm:@nolyfill/safe-buffer@^1",
      "safe-regex-test": "npm:@nolyfill/safe-regex-test@^1",
      "safer-buffer": "npm:@nolyfill/safer-buffer@^1",
      "string.prototype.includes": "npm:@nolyfill/string.prototype.includes@^1",
      "string.prototype.repeat": "npm:@nolyfill/string.prototype.repeat@^1",
      "string.prototype.trimend": "npm:@nolyfill/string.prototype.trimend@^1"
    }
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
}
